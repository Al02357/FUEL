<launch>
    <!-- Launch Gazebo Simulation -->

	<!-- <arg name="world" default="$(find prometheus_gazebo)/worlds/indoor_race_track.world"/> -->
    
    <include file="$(find prometheus_gazebo)/launch_basic/sitl.launch">
		<arg name="world" value="$(find rrt_exploration_tutorials)/launch/includes/worlds/maze_large.world"/>
		<arg name="sdf" value="$(find prometheus_gazebo)/amov_models/p450_D435i/p450_D435i.sdf"/>
		<arg name="model" value="p450_D435i"/>
		<arg name="x" value="2.0"/>
		<arg name="y" value="1.0"/>
		<arg name="z" value="0"/>
    </include>

    <include file="$(find prometheus_gazebo)/launch_basic/depth_proc.launch"/>

</launch>