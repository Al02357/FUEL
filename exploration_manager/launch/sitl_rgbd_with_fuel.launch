<launch>
    <!-- Launch Gazebo Simulation -->

	<!-- <arg name="world" default="$(find fuel_in_gazebo)/worlds/indoor_race_track.world"/> -->
    
    <include file="$(find fuel_in_gazebo)/launch/sitl.launch">
		<arg name="world" value="$(find fuel_in_gazebo)/worlds/room_mini.world"/>
		<arg name="sdf" value="$(find fuel_in_gazebo)/models/p450_D435i/p450_D435i.sdf"/>
		<arg name="model" value="p450_D435i"/>
		<arg name="x" value="-1.0"/>
		<arg name="y" value="0.0"/>
		<arg name="z" value="0"/>
    </include>

    <include file="$(find fuel_in_gazebo)/launch/depth_proc.launch"/>

</launch>